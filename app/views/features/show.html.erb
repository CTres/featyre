<div class="container">
  <div class="row" id="feature-row">
    <div class="span8 feature-inner">
  	  <div id="show_feature_title">
        <%= @feature.title %>
      </div>
      <div id="show_feature_subtitle">
        <%= @feature.subtitle %>
      </div>
  	  <div class="show_feature" id="feature_tag_list">
       <%= @feature.tag_list %>
      </div>
      <div class="feature_body">
  	    <%= @feature.text.html_safe %>
      </div>
    </div>
    <div class="collaborators span2 offset1">
        <legend> Collaborators </legend>
        <% @feature.collaborator_list.each do |user| %>
  		    <%= link_to user.username, user_path(user) %>
  		  <% end %>
      </div>
  </div>	
<!--     	<p>
    	  <%= link_to "Edit", edit_feature_path(@feature) %> |
    	  <%= link_to "Back to Features", features_path %>
    	</p> -->
</div>